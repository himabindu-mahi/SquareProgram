import java.io.*;
import java.util.*;
import java.lang.*; 


class Sample                               // Class declaration
{

	public void squareRoot(double n)       //Declare a method
	{
		
		
	double res=Math.sqrt(n);              //Define Method body  
		System.out.println("Square Root of a Given Number:  " +res);     //Displays Square root of the number
	}
	public void cubeRoot(double n)         //Declare a method
	{
				
		double result=Math.cbrt(n);
		System.out.println("Cube Root of a Given Number:   " +result);   //Displays Cube Root of the Number
	}
	
}
class SquareCubeRoot extends Sample
{
	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);       //Takes input values from the keyboard 
		System.out.println("Enter The Number");
		double n=sc.nextDouble();                  // Assign that input value to the n 
		
		SquareCubeRoot scr=new SquareCubeRoot();    // Object creation 
		scr.squareRoot(n);                          // Accessing methods through the objects
		scr.cubeRoot(n);
		
	}
}      
/*Output :-

Enter The Number
9
Square Root of a Given Number:  3.0
Cube Root of a Given Number:   2.080083823051904

*/
                               
 
